{
  "name": "gmail-customization-backend",
  "engines": {
    "node": "12.18.3"
  },
  "version": "0.1.0",
  "description": "Gmail Customization Backend with TypeScript",
  "repository": {
    "type": "git",
    "url": "https://github.com/AdoNaranjo/gmail-customization-backend"
  },
  "author": "Adolfo Naranjo",
  "scripts": {
    "start": "yarn run serve",
    "build": "rm -rf dist/* && yarn run build-ts && yarn run lint",
    "restart-serve": "yarn install && yarn run build && pm2 restart pm2.config.yml",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\"  \"yarn run watch-ts\" \"yarn run watch-node\"",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha --timeout 5000 -r ts-node/register 'test/**/*.test.ts'",
    "test-dev": "nodemon --watch . --ext ts --exec \"mocha --timeout 5000 -r ts-node/register test/**/*.test.ts\"",
    "watch-test": "yarn run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts,d.ts}\" --quiet --fix",
    "debug": "yarn run build && yarn run watch-debug",
    "serve-debug": "nodemon --inspect=9223 dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn run watch-ts\" \"yarn run serve-debug\" "
  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint \"**/*.{js,ts,d.ts}\" --quiet"
    }
  },
  "_moduleAliases": {
    "root": "dist"
  },
  "dependencies": {
    "@google-cloud/pubsub": "^2.5.0",
    "@sendgrid/mail": "^7.2.6",
    "@types/backoff": "^2.5.1",
    "@types/connect-mongo": "^3.1.3",
    "@types/cors": "^2.8.8",
    "@types/json2csv": "^5.0.2",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mjml": "^4.0.4",
    "@types/mongoose": "^5.10.4",
    "@types/nodemailer": "^6.4.0",
    "@types/puppeteer": "^5.4.3",
    "@types/request": "^2.48.5",
    "async": "^3.1.0",
    "aws-sdk": "^2.894.0",
    "axios": "^0.20.0",
    "body-parser": "^1.19.0",
    "bullmq": "^1.14.7",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "compression": "^1.7.4",
    "connect-mongo": "^4.4.1",
    "cors": "^2.8.5",
    "coveralls": "^3.1.0",
    "csv-parser": "^3.0.0",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "extract-zip": "^2.0.1",
    "fs": "^0.0.1-security",
    "grpc": "^1.24.3",
    "http-status-codes": "^1.4.0",
    "json2csv": "^5.0.6",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mjml": "^4.6.3",
    "module-alias": "^2.2.2",
