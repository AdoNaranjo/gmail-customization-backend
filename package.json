    "mongoose": "^5.12.3",
    "mongoose-sequence": "^5.3.1",
    "nlcst-to-string": "^2.0.4",
    "nodejs-file-downloader": "^4.5.3",
    "nodemailer": "^6.4.11",
    "nylas": "^5.2.0",
